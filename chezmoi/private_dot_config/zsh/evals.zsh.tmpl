# smartcache evals

[[ command -v brew ]] && smartcache eval brew shellenv
[[ command -v chezmoi ]] && smartcache comp chezmoi completion zsh
[[ command -v direnv ]] && smartcache eval direnv hook zsh
[[ command -v fzf ]] && smartcache eval fzf --zsh
[[ command -v mcfly ]] && smartcache eval mcfly init zsh
[[ command -v mcfly-fzf ]] && smartcache eval mcfly-fzf init zsh
[[ command -v mise ]] && smartcache eval mise activate zsh
[[ command -v starship ]] && smartcache eval starship init zsh
[[ command -v thefuck ]] && smartcache eval thefuck --alias
[[ command -v zoxide ]] && smartcache eval zoxide init zsh
