# smartcache evals
{{- if lookPath "brew" }}
smartcache eval brew shellenv
{{- end }}
{{- if lookPath "chezmoi" }}
smartcache comp chezmoi completion zsh
{{- end }}
{{- if lookPath "direnv" }}
smartcache eval direnv hook zsh
{{- end }}
{{- if lookPath "fzf" }}
smartcache eval fzf --zsh
{{- end }}
{{- if lookPath "mcfly" }}
smartcache eval mcfly init zsh
{{- end }}
{{- if lookPath "mcfly-fzf" }}
smartcache eval mcfly-fzf init zsh
{{- end }}
{{- if lookPath "mise" }}
smartcache eval mise activate zsh
{{- end }}
{{- if lookPath "starship" }}
smartcache eval starship init zsh
{{- end }}
{{- if lookPath "thefuck" }}
smartcache eval thefuck --alias
{{- end }}
{{- if lookPath "zoxide" }}
smartcache eval zoxide init zsh
{{- end }}
